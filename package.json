{
  "name": "@johngw/timeline",
  "version": "3.0.1",
  "description": "Parse a stream-like timeline of values",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/es/index.js",
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/common/index.js"
    },
    "./Timeline": {
      "import": "./dist/es/Timeline.js",
      "require": "./dist/common/Timeline.js"
    },
    "./TimelineItem": {
      "import": "./dist/es/TimelineItem/TimelineItem.js",
      "require": "./dist/common/TimelineItem/TimelineItem.js"
    },
    "./TimelineItemBoolean": {
      "import": "./dist/es/TimelineItem/TimelineItemBoolean.js",
      "require": "./dist/common/TimelineItem/TimelineItemBoolean.js"
    },
    "./TimelineItemClose": {
      "import": "./dist/es/TimelineItem/TimelineItemClose.js",
      "require": "./dist/common/TimelineItem/TimelineItemClose.js"
    },
    "./TimelineItemDash": {
      "import": "./dist/es/TimelineItem/TimelineItemDash.js",
      "require": "./dist/common/TimelineItem/TimelineItemDash.js"
    },
    "./TimelineItemDefault": {
      "import": "./dist/es/TimelineItem/TimelineItemDefault.js",
      "require": "./dist/common/TimelineItem/TimelineItemDefault.js"
    },
    "./TimelineItemError": {
      "import": "./dist/es/TimelineItem/TimelineItemError.js",
      "require": "./dist/common/TimelineItem/TimelineItemError.js"
    },
    "./TimelineItemNeverReach": {
      "import": "./dist/es/TimelineItem/TimelineItemNeverReach.js",
      "require": "./dist/common/TimelineItem/TimelineItemNeverReach.js"
    },
    "./TimelineItemNull": {
      "import": "./dist/es/TimelineItem/TimelineItemNull.js",
      "require": "./dist/common/TimelineItem/TimelineItemNull.js"
    },
    "./TimelineItemTimer": {
      "import": "./dist/es/TimelineItem/TimelineItemTimer.js",
      "require": "./dist/common/TimelineItem/TimelineItemTimer.js"
    }
  },
  "scripts": {
    "build": "yarn clean && tsc && tsc --project tsconfig.common.json",
    "clean": "rimraf dist",
    "start": "concurrently --names es,common 'yarn tsc --watch --preserveWatchOutput' 'yarn tsc --watch --preserveWatchOutput --project tsconfig.common.json'",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johngeorgewright/timeline.git"
  },
  "keywords": [],
  "author": "John Wright <johngeorge.wright@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johngeorgewright/timeline/issues"
  },
  "homepage": "https://github.com/johngeorgewright/timeline#readme",
  "devDependencies": {
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-conventional": "17.6.5",
    "@commitlint/types": "17.4.4",
    "@jest/types": "29.5.0",
    "@semantic-release/commit-analyzer": "10.0.0",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.0.2",
    "@semantic-release/release-notes-generator": "11.0.2",
    "@types/jest": "29.5.2",
    "@types/js-yaml": "4.0.5",
    "@types/node": "18.16.16",
    "@types/semantic-release": "20.0.1",
    "@types/source-map-support": "0.5.6",
    "commitizen": "4.3.0",
    "concurrently": "8.1.0",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "rimraf": "5.0.1",
    "semantic-release": "21.0.3",
    "source-map-support": "0.5.21",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.3"
  },
  "lint-staged": {
    "*.{md,json,js,jsx,ts,tsx,yml,yaml}": [
      "yarn prettier --ignore-path .gitignore --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "js-yaml": "4.1.0",
    "tslib": "2.5.3"
  },
  "packageManager": "yarn@3.6.0"
}