{
  "name": "@johngw/timeline",
  "version": "4.0.0",
  "description": "Parse a stream-like timeline of values",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js",
    "./Timeline": "./dist/Timeline.js",
    "./TimelineItem": "./dist/TimelineItem/TimelineItem.js",
    "./TimelineItemBoolean": "./dist/TimelineItem/TimelineItemBoolean.js",
    "./TimelineItemClose": "./dist/TimelineItem/TimelineItemClose.js",
    "./TimelineItemDash": "./dist/TimelineItem/TimelineItemDash.js",
    "./TimelineItemDefault": "./dist/TimelineItem/TimelineItemDefault.js",
    "./TimelineItemError": "./dist/TimelineItem/TimelineItemError.js",
    "./TimelineItemInstance": "./dist/TimelineItem/TimelineItemInstance.js",
    "./TimelineItemNeverReach": "./dist/TimelineItem/TimelineItemNeverReach.js",
    "./TimelineItemNull": "./dist/TimelineItem/TimelineItemNull.js",
    "./TimelineItemTimer": "./dist/TimelineItem/TimelineItemTimer.js"
  },
  "scripts": {
    "build": "bun run clean && tsc",
    "clean": "rimraf dist",
    "prepare": "husky install",
    "start": "tsc --watch --preserveWatchOutput"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johngeorgewright/timeline.git"
  },
  "keywords": [],
  "author": "John Wright <johngeorge.wright@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johngeorgewright/timeline/issues"
  },
  "homepage": "https://github.com/johngeorgewright/timeline#readme",
  "devDependencies": {
    "@commitlint/cli": "20.3.1",
    "@commitlint/config-conventional": "20.3.1",
    "@commitlint/types": "20.3.1",
    "@types/bun": "latest",
    "@types/js-yaml": "4.0.9",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.8.1",
    "rimraf": "6.1.2",
    "typescript": "5.9.3"
  },
  "lint-staged": {
    "*.{md,json,js,jsx,ts,tsx,yml,yaml}": [
      "bunx prettier --ignore-path .gitignore --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@johngw/outerface": "^2.0.2",
    "js-yaml": "^4.1.0",
    "tslib": "^2.6.3"
  }
}
